{
    "name": "blackajte/v3-services",
    "type": "symfony-bundle",
    "description": "V3 Services bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Blackajte"
        },
        {
            "name": "Thibaut OWCARZ",
            "email": "thibaut@owczcarz.fr"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "doctrine/collections": "^1.6",
        "symfony/dependency-injection": "^5.0",
        "symfony/http-foundation": "^5.0"
    },
    "require-dev": {
        "symfony/dotenv": "*",
        "phing/phing": "^2.16",
        "phpmd/phpmd": "^2.6",
        "sebastian/phpcpd": "^4.1",
        "squizlabs/php_codesniffer": "^3.3",
        "symfony/phpunit-bridge": "*"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Blackajte\\ServicesBundle\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0"
        }
    }
}
